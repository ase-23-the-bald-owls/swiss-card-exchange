{
  "name": "app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "npm run eslint && npm run format",
    "lint-check": "npm run eslint-check && npm run format-check",
    "eslint": "eslint '**/*.{js,jsx,ts,tsx}' && tsc",
